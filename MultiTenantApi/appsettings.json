{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "common",
    "ClientId": "00000000-0000-0000-0000-000000000000",
    "Audience": "api://00000000-0000-0000-0000-000000000000"
  },
  "MultiTenant": {
    "AllowAnyTenant": false,
    "AllowedTenants": [
      "11111111-1111-1111-1111-111111111111"
    ]
  },
  "AuthZ": {
    "DocumentsReadScope": "Documents.Read",
    "ReportsReadAllScope": "Reports.Read.All",
    "ReportsReadAllAppRole": "Reports.Read.All"
  },
  "SyntheticId": {
    "KeyBase64": "REPLACE_WITH_32_BYTE_RANDOM_BASE64",
    "Namespace": "prod"
  },
  "TokenHardening": {
    "MaxAccessTokenAgeMinutes": 15,
    "ClockSkewSeconds": 30,
    "EnableJtiReplayProtection": true,
    "JtiCacheMinutes": 20
  },
  "RateLimiting": {
    "PerIdentityPerMinute": 300,
    "BurstPer10Seconds": 50
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}